{
    "name": "acquia/blt-project",
    "type": "project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "license": "GPL-2.0-only",
    "require": {
        "php": ">=7.2",
        "acquia/blt": "^11.1.0",
        "acquia/composer-scaffold": "dev-master",
        "acquia/drupal-spec-tool": "*",
        "acquia/lightning": "dev-8.x-4.x",
        "acquia/memcache-settings": "*",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acquia_purge": "^1.0-beta3",
        "drupal/admin_toolbar": "^2.0",
        "drupal/allowed_formats": "^1.2",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/block_blacklist": "^1.0",
        "drupal/chosen": "^2.0",
        "drupal/cog": "^1.0.0",
        "drupal/config_filter": "^1.0",
        "drupal/config_ignore": "^2.2",
        "drupal/config_split": "^1.0.0",
        "drupal/cookieconsent": "^1.4",
        "drupal/core-recommended": "^8.8.0",
        "drupal/default_content": "^1.0@alpha",
        "drupal/facets": "^1.4",
        "drupal/features": "^3.8.0",
        "drupal/field_group": "^3.0",
        "drupal/focal_point": "^1.2",
        "drupal/google_tag": "^1.3",
        "drupal/honeypot": "^1.30",
        "drupal/paragraphs": "^1.10",
        "drupal/pathauto": "^1.6",
        "drupal/robotstxt": "^1.3",
        "drupal/schema_metatag": "^1.4",
        "drupal/search_api": "^1.0",
        "drupal/search_api_solr": "^3.0",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/shield": "^1.2.0",
        "drupal/simple_sitemap": "^3.5",
        "drupal/smart_trim": "^1.2",
        "drupal/tmgmt": "^1.10",
        "drupal/twig_tweak": "^2.4",
        "drupal/varnish_purger": "^1.11",
        "drupal/webform": "^5.8"
    },
    "require-dev": {
        "acquia/blt-require-dev": "11.x-dev",
        "drupal/devel": "^2.1",
        "drupal/qa_accounts": "^1.0@alpha",
        "drupal/stage_file_proxy": "^1.0",
        "mglaman/phpstan-drupal": "^0.12.2",
        "weitzman/drupal-test-traits": "^1.3"
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true,
        "discard-changes": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "./docroot"
            },
            "allowed-packages": {
                "drupal/core": "*"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                }
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/default_content": {
                "2989887 - Exported revision IDs are not respected - does not work with entity reference revision fields like Paragraph": "https://www.drupal.org/files/issues/2018-08-06/exported_revision_ids-2989887-6.patch"
            },
            "drupal/better_normalizers": {
                "Replace existing files if they exist": "https://www.drupal.org/files/issues/2020-01-07/2864407-file-replace-20.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    }
}
